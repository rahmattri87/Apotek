	<queryString><![CDATA[SELECT
     penjualan.`nota` AS penjualan_nota,
     penjualan.`tgl` AS penjualan_tgl,
     penjualan.`nama` AS penjualan_nama,
     penjualan.`alamat` AS penjualan_alamat,
     penjualan.`telp` AS penjualan_telp,
     penjualan.`grand` AS penjualan_grand,
     penjualan.`user` AS penjualan_user,
     penjualan_detail.`nota` AS penjualan_detail_nota,
     penjualan_detail.`kdobat` AS penjualan_detail_kdobat,
     penjualan_detail.`harga` AS penjualan_detail_harga,
     penjualan_detail.`jumlah` AS penjualan_detail_jumlah,
     penjualan_detail.`total` AS penjualan_detail_total,
     obat.`nmobat` AS obat_nmobat,
     obat.`kdobat` AS obat_kdobat
FROM
     `penjualan_detail` penjualan_detail INNER JOIN `penjualan` penjualan ON penjualan_detail.`nota` = penjualan.`nota`
     INNER JOIN `obat` obat ON penjualan_detail.`kdobat` = obat.`kdobat`
WHERE
     penjualan.nota BETWEEN $P{awal} AND $P{akhir}]]></queryString>

